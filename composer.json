{
    "name": "bernardosequeir/laravel-table-view",
    "description": "Laravel 5 Package for easily displaying table views for Eloquent Collections with search and sort functionality built in.",
    "keywords": [
        "search",
        "sort",
        "pagination",
        "data table",
        "table view",
        "laravel",
        "laravel 5"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "bernardosequeir\\LaravelTableView\\": "src/"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "illuminate/support": "~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpspec/phpspec": "~2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "bernardosequeir\\LaravelTableView\\Tests\\": "tests/"
        }
    }
}